<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.nes.customgooglelauncher.ui.widget.CustomTextView
        android:id="@+id/item_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Regional"
        android:paddingStart="@dimen/home_out_padding_start"
        android:paddingEnd="@dimen/home_out_padding_start"
        android:textColor="@color/home_item_title_color"
        android:textSize="@dimen/home_item_title_sp"
        app:myTypeface="ROBOTO_LIGHT"
        android:paddingBottom="@dimen/home_item_item_progressbar_magin"/>
    <com.nes.customgooglelauncher.ui.widget.FirstColumnRecyclerView
        android:id="@+id/item_recyclerView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingStart="@dimen/home_out_padding_start"
        android:paddingEnd="@dimen/home_out_padding_start"
        android:clipChildren="false"
        android:clipToPadding="false"
        />

</merge>
